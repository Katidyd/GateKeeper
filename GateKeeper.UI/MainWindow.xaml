<Window x:Class="GateKeeper.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GateKeeper.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="700
        " Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
                <RowDefinition Height="28"></RowDefinition>
                <RowDefinition Height="28"></RowDefinition>
            <RowDefinition Height="28"></RowDefinition>
            <RowDefinition Height="28"></RowDefinition>
            <RowDefinition Height="28"></RowDefinition>
            <RowDefinition Height="28"></RowDefinition>
            <RowDefinition Height="28"></RowDefinition>
            <RowDefinition Height="28"></RowDefinition>
            <RowDefinition Height="28"></RowDefinition>
            <RowDefinition Height="28"></RowDefinition>
            <RowDefinition Height="28"></RowDefinition>
            <RowDefinition Height="28"></RowDefinition>
            <RowDefinition Height="28"></RowDefinition>
            <RowDefinition Height="28"></RowDefinition>
            <RowDefinition Height="28"></RowDefinition>
         </Grid.RowDefinitions>
        <Label Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="4">Current Shift</Label>
        <Button Grid.Column="1" Grid.Row="1">-</Button>
        <Button Grid.Column="3" Grid.Row="1">+</Button>
        <Button Grid.Column="1" Grid.Row="2">-</Button>
        <Button Grid.Column="3" Grid.Row="2">+</Button>
        <Button Grid.Column="1" Grid.Row="3">-</Button>
        <Button Grid.Column="3" Grid.Row="3">+</Button>
        <Button Grid.Column="1" Grid.Row="4">-</Button>
        <Button Grid.Column="3" Grid.Row="4">+</Button>
        <Button Grid.Column="1" Grid.Row="5">-</Button>
        <Button Grid.Column="3" Grid.Row="5">+</Button>
        <Button Grid.Column="1" Grid.Row="6">-</Button>
        <Button Grid.Column="3" Grid.Row="6">+</Button>
        <Button Grid.Column="1" Grid.Row="7">-</Button>
        <Button Grid.Column="3" Grid.Row="7">+</Button>
        <Button Grid.Column="1" Grid.Row="8">-</Button>
        <Button Grid.Column="3" Grid.Row="8">+</Button>
        <Button Grid.Column="1" Grid.Row="9">-</Button>
        <Button Grid.Column="3" Grid.Row="9">+</Button>
        <Button Grid.Column="1" Grid.Row="10">-</Button>
        <Button Grid.Column="3" Grid.Row="10">+</Button>
        <Button Grid.Column="1" Grid.Row="11">-</Button>
        <Button Grid.Column="3" Grid.Row="11">+</Button>
        <Button Grid.Column="1" Grid.Row="12">-</Button>
        <Button Grid.Column="3" Grid.Row="12">+</Button>
        <Button Grid.Column="1" Grid.Row="13">-</Button>
        <Button Grid.Column="3" Grid.Row="13">+</Button>
        <Button Grid.Column="1" Grid.Row="14">-</Button>
        <Button Grid.Column="3" Grid.Row="14">+</Button>
        <Label Grid.Column="0" Grid.Row="1" >Adult Overnight</Label>
        <Label Grid.Column="0" Grid.Row="2" >Member Overnight</Label>
        <Label Grid.Column="0" Grid.Row="3" >Youth (13-17) Overnight</Label>
        <Label Grid.Column="0" Grid.Row="4" >Child  (6-12) Overnight</Label>
        <Label Grid.Column="0" Grid.Row="5" >Child  (0-5) Overnight</Label>
        <Label Grid.Column="0" Grid.Row="6" >Adult Daytrip</Label>
        <Label Grid.Column="0" Grid.Row="7" >Member Daytrip</Label>
        <Label Grid.Column="0" Grid.Row="8" >Youth (13-17) Daytrip</Label>
        <Label Grid.Column="0" Grid.Row="9" >Child (6-12) Daytrip</Label>
        <Label Grid.Column="0" Grid.Row="10" >Child (0-5) Daytrip</Label>
        <Label Grid.Column="0" Grid.Row="11" >Daytrip Conversion</Label>
        <Label Grid.Column="0" Grid.Row="12" >Local</Label>
        <Label Grid.Column="0" Grid.Row="13" >Comp</Label>
        <Label Grid.Column="0" Grid.Row="14" >Other</Label>
        <Label Grid.Column="2" Grid.Row="1" ></Label>
        <Label Grid.Column="2" Grid.Row="2" ></Label>
        <Label Grid.Column="2" Grid.Row="3" ></Label>
        <Label Grid.Column="2" Grid.Row="4" ></Label>
        <Label Grid.Column="2" Grid.Row="5" ></Label>
        <Label Grid.Column="2" Grid.Row="6" ></Label>
        <Label Grid.Column="2" Grid.Row="7" ></Label>
        <Label Grid.Column="2" Grid.Row="8" ></Label>
        <Label Grid.Column="2" Grid.Row="9" ></Label>
        <Label Grid.Column="2" Grid.Row="10" ></Label>
        <Label Grid.Column="2" Grid.Row="11" ></Label>
        <Label Grid.Column="2" Grid.Row="12" ></Label>
        <Label Grid.Column="2" Grid.Row="13" ></Label>
        <Label Grid.Column="2" Grid.Row="14" ></Label>
    </Grid>
</Window>
